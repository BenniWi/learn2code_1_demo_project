{
"markdown-pdf.styles": [
    "markdown-pdf.css",
  ],
"markdown-pdf.stylesRelativePathFile": true,
"markdown-pdf.highlightStyle": "github.css",
"markdown-pdf.displayHeaderFooter": true,
"markdown-pdf.footerTemplate": "<div style=\"font-size: 8px; color: green; margin-left: auto; margin-right: 30px\"> <span class='pageNumber'></span>/<span class='totalPages'></span></div>",
"markdown-pdf.headerTemplate": " ",


"markdown.styles": [
    "docs/markdown-pdf.css"
  ],

  "cSpell.language": "en,de-de",
  "files.associations": {
    "*.tcc": "c",
    "library.h": "c",
    "errno.h": "c",
    "string.h": "c"
  },


  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[cpp]": {
      "editor.defaultFormatter": "xaver.clang-format"
  },
  "clang-tidy.buildPath": "build",
  "clang-tidy.fixOnSave": true,
  "clang-tidy.checks": ["*"]
}
